{
  "name": "lautfm-adminapi",
  "version": "0.0.4",
  "description": "JS wrapper for admin laut.fm api",
  "main": "./lib/index.js",
  "scripts": {
    "build:docs": "npx documentation build src/** -f html -o docs",
    "build": "npx babel src -d lib",
    "test": "jest",
    "preversion": "npm run test",
    "version": "npm run build && npm run build:docs && git add .",
    "postversion": "get push && git push --tags && npm publish"
  },
  "keywords": [
    "lautfm"
  ],
  "repository": "github:fvhockney/lautfm-adminapi",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "author": "Vern Hockney <vern@laut.fm>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "axios": "^0.18.0",
    "babel-jest": "^24.3.1",
    "documentation": "^9.3.1",
    "jest": "^24.3.1"
  },
  "peerDependencies": {
    "axios": "^0.18.0",
    "@babel/polyfill": "^7.2.5"
  },
  "dependencies": {}
}
