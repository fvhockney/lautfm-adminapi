{
  "name": "lautfm-adminapi",
  "version": "0.0.9",
  "description": "JS wrapper for admin laut.fm api",
  "main": "cjs/",
  "module": "esm/",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "npx babel --delete-dir-on-start --env-name cjs -d cjs/ src/",
    "build:cjs": "npx babel --delete-dir-on-start -d esm/ src/",
    "docs": "npx documentation build src/** -f html -o docs",
    "test": "jest",
    "preversion": "npm run test",
    "version": "git add .",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "lautfm"
  ],
  "repository": "github:fvhockney/lautfm-adminapi",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "author": "Vern Hockney <vern@laut.fm>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "axios": "^0.18.1",
    "babel-jest": "^24.9.0",
    "documentation": "^9.3.1",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3"
  },
  "peerDependencies": {
    "axios": "^0.18.0",
    "@babel/polyfill": "^7.2.5"
  },
  "dependencies": {}
}
